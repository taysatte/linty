{
  "name": "linty",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "start:db": "supabase stop && supabase start",
    "build": "pnpm prisma:generate:prod && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "prisma:generate": "pnpm dlx dotenv-cli -e .env.development -- prisma generate",
    "prisma:migrate": "pnpm dlx dotenv-cli -e .env.development -- prisma migrate dev",
    "prisma:studio": "pnpm dlx dotenv-cli -e .env.development -- prisma studio",
    "prisma:db:push": "pnpm dlx dotenv-cli -e .env.development -- prisma db push",
    "prisma:seed": "pnpm dlx dotenv-cli -e .env.development -- tsx prisma/seed.ts",
    "prisma:seed:prod": "pnpm dlx dotenv-cli -e .env.production -- tsx prisma/seed.ts",
    "prisma:generate:prod": "pnpm dlx dotenv-cli -e .env.production -- prisma generate",
    "prisma:migrate:prod": "pnpm dlx dotenv-cli -e .env.production -- prisma migrate deploy",
    "prisma:db:push:prod": "pnpm dlx dotenv-cli -e .env.production -- prisma db push",
    "postinstall": "pnpm prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.81.1",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "monaco-editor": "^0.54.0",
    "motion": "^12.23.24",
    "next": "15.5.9",
    "next-themes": "^0.4.6",
    "pnpm": "^10.20.0",
    "radix-ui": "^1.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@prisma/client": "^7.2.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20.19.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.39.0",
    "eslint-config-next": "15.5.5",
    "prisma": "^7.2.0",
    "supabase": "^2.54.11",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
